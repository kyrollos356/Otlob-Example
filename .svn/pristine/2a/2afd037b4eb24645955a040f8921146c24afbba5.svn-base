@charset "ISO-8859-1";

.title {
	color: #333333;
	font-size: 33px;
	font-family: 'Quicksand';
	font-weight: bold;
}

.name{
	color: #93b32d;
	font-size: 33px;
	font-family: 'Quicksand';
	font-weight: bold;
	
}
.username {
	margin-left: 20px;
	color: #333333;
	font-size: 23px;
	font-family: 'Quicksand';
	font-weight: bold;
}

.skypeemail {
